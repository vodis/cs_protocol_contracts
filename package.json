{
  "name": "cs_protocol_contracts",
  "version": "1.0.0",
  "description": "Diamond contracts for CraftScript protocol",
  "files": [
    "/contracts/**/*.sol"
  ],
  "scripts": {
    "compile": "npx hardhat compile",
    "local-network:run": "npx hardhat node",
    "local-network:deploy": "npx hardhat run --network localhost scripts/deployAll.js",
    "solhint:check": "solhint \"contracts/**/*.sol\"",
    "prettier:contracts-fix": "prettier --config .prettierrc --write --plugin=prettier-plugin-solidity '{contracts,src}/**/*.sol'",
    "prettier:scripts-fix": "prettier --config .prettierrc --write '{scripts,migrations,test,src}/**/*.js'",
    "tasks:diamond-abi": "npx hardhat diamondABI",
    "test": "npx hardhat test --network hardhat",
    "prepare": "husky install"
  },
  "author": "vodis",
  "license": "ISC",
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "dotenv": "^16.3.1",
    "hardhat": "^2.19.4",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^8.0.3",
    "prettier": "^3.1.1",
    "prettier-plugin-solidity": "^1.3.1",
    "solhint": "^4.1.1",
    "solhint-plugin-prettier": "^0.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
